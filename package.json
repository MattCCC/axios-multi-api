{
  "version": "1.3.3",
  "license": "MIT",
  "name": "axios-multi-api",
  "author": "Matt Czapli≈Ñski <deindesign.pl@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MattCCC/axios-multi-api.git"
  },
  "main": "dist/browser/index.esm.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "axios-api",
    "axios-api-handler",
    "axios-multi-api",
    "api",
    "api-handler",
    "browser",
    "node"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "node ./scripts/build.js",
    "build:browser": "tsdx build --target web --format esm --name index --experimental-modules",
    "build:node": "tsdx build --target node --format cjs --name index",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "release": "npm version patch && git push --tags",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/browser/index.esm.js",
  "size-limit": [
    {
      "path": "dist/axios-multi-api.cjs.production.min.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/axios-multi-api.esm.js",
      "limit": "50 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/jest": "^29.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "promise-any": "0.2.0",
    "rollup-plugin-bundle-imports": "^1.5.1",
    "size-limit": "^8.0.1",
    "ts-jest": "^29.0.0-next.0",
    "tsdx": "0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "js-magic": "^1.2.3"
  }
}
